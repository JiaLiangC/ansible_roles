{{nginx_log_dir}}/*log {
    daily
    rotate 10
    missingok
    notifempty
    compress
    size 100M
    sharedscripts
    postrotate
        /bin/kill -USR1 $(cat {{nginx_var_dir}}/nginx.pid 2>/dev/null) 2>/dev/null || :
        dt=$(date -d "8 day ago" +%Y%m%d)
        test -e {{nginx_log_dir}}/${dt} || mkdir -p {{nginx_log_dir}}/${dt}
        mv /data/apps/log/nginx/*.log-${dt}* {{nginx_log_dir}}/${dt} 
    endscript
}
