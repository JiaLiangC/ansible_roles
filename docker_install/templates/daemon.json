{
  "insecure-registries": [
    "{{docker_insecure_registry}}"
  ],
  "hosts": [
    "unix:///var/run/docker.sock"
  ],
  "graph": "{{docker_data_dir}}",
  "exec-opts": [
     "native.cgroupdriver=systemd"
  ],
  "log-driver": "json-file",
  "log-opts": {
    "max-size": "100m",
    "max-file": "10"
  }
}
