{{log_dir}}/*.log {
    weekly
    rotate 10
    copytruncate
    size 100M
    dateext
    notifempty
    missingok
    postrotate
        log_dir={{archive_log_dir}}
        test -e ${log_dir} || mkdir -p ${log_dir}
        dt=$(date -d "8 day ago" +%Y%m%d)
        mv {{log_dir}}/*.log-${dt} ${log_dir}
    endscript
}
